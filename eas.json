{
  "cli": {
    "version": ">= 14.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_GRAPHQL_REST_URL": "https://nutrition.athvio.com/graphql",
        "EXPO_PUBLIC_GRAPHQL_WS_URL": "ws://nutrition.athvio.com/graphql"
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
